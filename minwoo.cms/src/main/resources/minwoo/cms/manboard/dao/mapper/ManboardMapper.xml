<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd"> 
<mapper namespace="minwoo.cms.manboard.dao.mapper.ManboardMapper">
	<resultMap id="userMap" type="minwoo.cms.manboard.domain.ManboardInfo">
		<id property="mabId" column="mab_id" />
		<result property="mabNm" column="mab_nm" />
		<result property="mabYn" column="mab_yn" />
	</resultMap>
	
	<select id="getBoardList" resultMap="userMap">
		select MAB_ID, MAB_NM from MAINBOARDS
	</select>	
	
	<select id="getBoardList2" resultType="minwoo.cms.manboard.domain.ManboardInfo">
		select MAB_ID, MAB_NM from MAINBOARDS
	</select>		
	
	<insert id="addUser" parameterType="minwoo.cms.manboard.domain.ManboardInfo">
		insert into users(USER_ID,USER_PWD,USER_MAIL,USER_BIRTH,USER_NM,USER_NIC) 
			values(
					 #{userId},#{userPwd},#{userMail},#{userBirth},#{userNm},#{userNic}
				)
	</insert>	
	
</mapper>